#!/usr/bin/env ruby
require 'pry'
require_relative '../lib/tic_tac_toe.rb'

def initialize(board = nil)
puts "Welcome to Tic Tac Toe!"
board = [" "," "," "," "," "," "," "," "," "]
puts "Where would you like to go?"
input = gets.strip
index = input_to_index(input)
move(board, index, value = "X")
display_board(board)
play
end
